@model SKBKontur.Treller.WebApplication.Models.TaskList.TaskListViewModel

@{
    ViewBag.Title = "Информация по задачам в трелло";
    Layout = "../Shared/_Layout.cshtml";
}

@section Header
{
    @Styles.Render("~/bundles/css/tasklist")
}

@section LayoutHeader
{
    @Html.Partial("Partial/BaseBlocks", Model.HeaderBlocks)
}

@Html.React("TaskListComponent", new { data = Model.TaskList })

@section FooterScripts
{
    @Scripts.Render(
        "~/bundles/js/react",
        "~/bunsles/jsx/tasklist")

    @Html.ReactInitJavaScript()
}
